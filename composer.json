{
    "name": "lekoala/silverstripe-cookieconsent",
    "description": "Integrate cookieconsent.com in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cookies",
        "cookieconsent",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": ">=7.2",
        "silverstripe/framework": "^4",
        "lekoala/silverstripe-defer-backend": "^1.1"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "cookieconsent",
        "expose": [
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\CookieConsent\\": "src/",
            "LeKoala\\CookieConsent\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-cookieconsent/issues"
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
